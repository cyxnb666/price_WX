page {
  height: 100%;
  width: 100%;
}
.pricingTask {
  height: calc(100% - 91px);
  width: 100%;
  background-color: #F2F2F2FF;
  padding: 20rpx;
  box-sizing: border-box;
}
.t-dropdown-menu__item{
  width: 160rpx !important;
}
.t-dropdown-item__body,.t-dropdown-item__scroll{
  height: 200px;
}
.queryBox{
  margin-bottom: 20rpx;
}
.pricingTaskItem{
  background-color: #FFFFFF;
  padding: 20rpx;
  box-sizing: border-box;
  box-shadow: 0 6rpx 12rpx rgba(0, 0, 0, 0.15);
  border-radius: 12rpx;
  margin-bottom: 20rpx;
}
.pricingTaskItem-header{
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.pricingTaskItem-header-left{
  color: #000000;
  font-size: 32rpx;
  font-weight: bold;
}
.pricingTaskItem-body-item{
  display: flex;
}
.pricingTaskItem-body-item-title{
  color: #666666;
  width: 210rpx;
  margin-right: 40rpx;
}
.pricingTaskItem-body-item-content{
  color: #666666;
}
.pricingTaskItem-header-right{
  background-color: #3693FF66;
  border-radius: 8rpx;
  color: #000000;
  font-weight: bold;
  padding: 0 12rpx;
}
.pricingTaskItem-header-right2{
  background-color: #CCEDAA;
  border-radius: 8rpx;
  color: #000000;
  font-weight: bold;
  padding: 0 12rpx;
}
.auditStaus{
  background-color: #CCEDAA !important;
}
.nothingMore{
  color: #666666;
  width: 100%;
  text-align: center;
}
.custom-navbar{
  --td-navbar-color: #fff !important;
  --td-navbar-bg-color: #7393FF !important;
}